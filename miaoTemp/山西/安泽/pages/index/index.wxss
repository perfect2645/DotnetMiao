.headbg {
    margin: 0;
    padding: 0;
    position: relative;
}

.headbg,.headbg image {
    height: 452rpx;
    width: 100%;
}

.headbg image,.type {
    position: absolute;
}

.type {
    box-sizing: border-box;
    color: #b3cfff;
    display: flex;
    font-size: 30rpx;
    width: 100%;
    z-index: 1;
}

.type_li {
    height: 70rpx;
    line-height: 70rpx;
    margin-left: 30rpx;
}

.white {
    color: #fff;
    font-weight: 700;
}

.makeTab_li_line {
    background-color: #fff;
    border-radius: 50rpx;
    height: 6rpx;
    margin: auto;
    width: 45rpx;
}

.banner_box {
    position: absolute;
    top: 90rpx;
    width: 100%;
    z-index: 2;
}

.banner {
    height: 452rpx;
    margin: auto;
    width: 95%;
}

.banner navigator {
    height: 100%;
    width: 100%;
}

.banner image {
    border-radius: 20rpx;
    height: 452rpx;
    width: 100%;
}

.userinfo {
    align-items: center;
    color: #aaa;
    display: flex;
    flex-direction: column;
}

.userinfo-avatar {
    border-radius: 50%;
    height: 128rpx;
    margin: 20rpx;
    overflow: hidden;
    width: 128rpx;
}

.usermotto {
    margin-top: 200px;
}

.banner_line {
    height: 110rpx;
    width: 100%;
}

.s_gg_content {
    margin: auto auto 20rpx;
    width: 100%;
}

.s_gg_content,.s_gg_content2 {
    background-color: #fdf9f6;
    min-height: 80rpx;
}

.s_gg_content2 {
    color: #ed9458;
    font-size: 30rpx;
    margin: auto;
    padding-bottom: 20rpx;
    padding-top: 20rpx;
    width: 95%;
}

.s_gg {
    background-color: #fff;
    border-radius: 10rpx;
    height: 80rpx;
    margin: auto auto 20rpx;
    width: 95%;
}

.s_gg_left {
    margin-left: 15rpx;
}

.s_gg_left,.s_gg_left image {
    float: left;
    height: 80rpx;
    width: 120rpx;
}

.s_gg_right {
    color: #313131;
    float: right;
    font-size: 30rpx;
    height: 80rpx;
    line-height: 80rpx;
    overflow: hidden;
    text-align: left;
    text-overflow: ellipsis;
    white-space: nowrap;
    width: 80%;
}

.s_list {
    border-radius: 10rpx;
}

.s_list,.s_list_content {
    background-color: #fff;
    margin: auto;
    min-height: 248rpx;
    width: 95%;
}

.s_list_top {
    border-bottom: 1rpx solid #e1e1e1;
    color: #3a3a3a;
    font-size: 35rpx;
    height: 90rpx;
    line-height: 90rpx;
    text-align: left;
    width: 100%;
}

.s_list_top image {
    float: left;
    height: 40rpx;
    margin-right: 25rpx;
    margin-top: 25rpx;
    width: 40rpx;
}

.makelist_li {
    background-color: #fff;
    height: 285rpx;
    margin: 15rpx auto auto;
    width: 100%;
}

.makelist_li_content {
    height: 248rpx;
    margin: auto;
    padding-top: 18rpx;
    width: 100%;
}

.makelist_li_left {
    float: left;
    height: 248rpx;
    width: 40%;
}

.makelist_li_left image {
    height: 248rpx;
    width: 248rpx;
}

.makelist_li_right {
    float: left;
    height: 248rpx;
    position: relative;
    width: 60%;
}

.makelist_li_right_title {
    color: #333;
    font-size: 30rpx;
    font-weight: 700;
    min-height: 60rpx;
    text-align: left;
    width: 100%;
}

.makelist_li_right_text {
    color: #959595;
    font-size: 24rpx;
    min-height: 40rpx;
    text-align: left;
    width: 100%;
}

.makelist_li_right_over {
    background-color: #ccc;
    border-radius: 8rpx;
    color: #fff;
    font-size: 24rpx;
    height: 40rpx;
    line-height: 40rpx;
    text-align: center;
    width: 90rpx;
}

.makelist_li_right_image {
    border-radius: 50rpx;
    bottom: 0;
    color: #fff;
    font-size: 30rpx;
    font-weight: 700;
    height: 70rpx;
    position: absolute;
    right: 0;
    text-align: center;
    width: 200rpx;
    z-index: 1;
}

.makelist_li_right_image text {
    line-height: 70rpx;
}

.n_lanse {
    background-color: #3178e4;
}

.n_huise {
    background-color: #ccc;
}

.makelist_li_right_image image {
    height: 28rpx;
    margin: 20rpx 10rpx auto auto;
    width: 28rpx;
}

.makelist_li_price {
    bottom: 0;
    height: 80rpx;
    left: 0rpx;
    position: absolute;
    width: 100%;
    z-index: 1;
}

.price_hx {
    color: #979797;
    display: none;
    font-size: 24rpx;
    text-decoration: line-through;
}

.price {
    bottom: 0;
    color: #3773d3;
    font-size: 30rpx;
    font-weight: 700;
    left: 0;
    position: absolute;
    z-index: 1;
}

.makelist_li_time {
    background-color: #edc57d;
    border-radius: 5rpx;
    color: #fff;
    display: inline-block;
    font-size: 24rpx;
    padding: 8rpx 10rpx;
    text-align: center;
}

.s_bottom {
    height: 80rpx;
    width: 100%;
}

.bg,.swiper-container {
    height: 100%;
    width: 100%;
}

.bg {
    background-color: #000;
    display: block;
    filter: alpha(opacity=70);
    left: 0;
    opacity: .7;
    position: fixed;
    top: 0;
    z-index: 666;
}

.tanchuang {
    background: #fff;
    border-radius: 20rpx;
    left: 10%;
    margin: 0 auto;
    min-height: 700rpx;
    position: fixed;
    top: 15%;
    width: 80%;
    z-index: 1000;
}

.tanchuang_top {
    margin: 50rpx auto auto;
    width: 165rpx;
}

.tanchuang image {
    margin: auto;
    width: 165rpx;
}

.tanchuang_content {
    color: #2a2a2a;
    font-size: 28rpx;
    height: 320rpx;
    margin: 20rpx auto auto;
    width: 95%;
}

.tanchuang_bottom {
    background-color: #5493fc;
    border-bottom-left-radius: 20rpx;
    border-bottom-right-radius: 20rpx;
    bottom: 0;
    color: #f8ffff;
    font-size: 30rpx;
    height: 80rpx;
    left: 0;
    line-height: 80rpx;
    margin: auto;
    position: absolute;
    text-align: center;
    width: 100%;
    z-index: 1;
}

.tanchuangyzm {
    background: #fff;
    border-radius: 20rpx;
    left: 10%;
    margin: 0 auto;
    min-height: 300rpx;
    position: fixed;
    top: 45%;
    width: 80%;
    z-index: 1000;
}

.tanchuangyzm_content {
    color: #2a2a2a;
    font-size: 28rpx;
    height: 100rpx;
    margin: auto;
    width: 90%;
}

.yzminput {
    border: 1rpx solid #ccc;
    border-radius: 10rpx;
    float: left;
    text-indent: 12rpx;
    width: 65%;
}

.yzm,.yzminput {
    height: 28px;
    margin-top: 70rpx;
}

.yzm {
    border-radius: 14px;
    float: right;
    width: 30%;
}

.tanchuangyzm_bottom {
    background-color: #5493fc;
    border-bottom-left-radius: 20rpx;
    border-bottom-right-radius: 20rpx;
    bottom: 0;
    color: #f8ffff;
    font-size: 30rpx;
    height: 80rpx;
    left: 0;
    line-height: 80rpx;
    margin: auto;
    position: absolute;
    text-align: center;
    width: 100%;
    z-index: 1;
}