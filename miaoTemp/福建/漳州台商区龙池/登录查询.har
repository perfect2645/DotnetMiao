\t\t\t}\r\n\t\t\t\r\n\t\t\tvar s = parseInt(time % 60)\r\n\t\t\ts=s+'\u79d2';\r\n\t\t\t\r\n\t\t\t\r\n\t\t\t// \u4f5c\u4e3a\u8fd4\u56de\u503c\u8fd4\u56de\r\n\t\t\treturn h+ m+ s;\r\n\t\t\t}\r\n\r\n \r\n\t\r\n\t\r\n  var webSocket2 = new YT_WebSocket('ws://121.40.67.146:15200/');\r\n\t\r\n\t\t webSocket2.onmessage=function(event){\r\n\t\t var data=event.data.split(\"|\");\r\n \r\n if(data[0]=='login'){\r\n window.location.replace(data[1]);\r\n }\r\n  if(data[0]=='cancel_login'){\r\n\t$('#loading').hide();\r\n\t$('#begin').show();\r\n\t\r\n\t// $('#begin').html('<span style=\"letter-spacing:1px;font-size:16px;font-weight:normal\" >\u8ddd\u79bb\u8fdb\u5165\u9884\u7ea6<br/><span style=\"font-size:16px\">'+getTime_str(qty)+'</span></span>');\r\n\t \r\n\t \r\n\t\r\n }\r\n \r\n \r\n\t\t };\t\r\n\t\t\r\n\t var is_begin=0;\r\n\t \r\n\tvar bw= $('#but_div').width(); \r\n\t$('#the-loading-demo').css('left', ((bw*0.5)-30)+'px');\r\n\t\r\n\t\r\n\t\r\n\t\r\n\t\r\n\t     \r\n  var getnow = new Date();  \r\n     var getdate = getnow.getDate();\r\n\t \r\n\t if(getdate==1 ||  getdate==15){\t\t \r\n  \r\n\t\r\n var jishi=setInterval(function() {\r\n  \r\n  \tvar now_time=get_time();\r\n\tvar qty=compareTime(now_time,begin_time);\r\n\t\r\n\tif(qty>0 && qty<xianzhi_time){\r\n $('#begin').html('<span style=\"letter-spacing:1px;font-size:16px;font-weight:normal\" >\u8ddd\u79bb\u8fdb\u5165\u9884\u7ea6<br/><span style=\"font-size:16px\">'+getTime_str(qty)+'</span></span>');\r\n   return ;\r\n\t}\r\n\t\r\n  \r\n\t $('#begin').html('\u8fdb\u5165\u9884\u7ea6');\r\n\t \r\n}, 1000);\r\n\r\n};\r\n\t\r\n\t\r\n\tfunction begin()\r\n\t{\r\n\r\n\t\tvar getnow = new Date();  \r\n\t\t var getdate = getnow.getDate();\r\n\t\t \r\n\t\t if(getdate==1 ||  getdate==15){\t\r\n\t\t \t\t \r\n\t\tvar now_time=get_time();\r\n\t\tvar qty=compareTime(now_time,begin_time); \r\n\t\t\r\n\t\tif(qty>0 && qty<xianzhi_time) return ;\r\n\t\t\r\n\r\n\t\t}\r\n \r\n\t\r\n\t$('#begin').hide();\r\n\t\t$('#loading').html('\u6b63\