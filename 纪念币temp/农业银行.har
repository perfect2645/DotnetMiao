}\r\n\r\n    function orgBind(parentCode, obj, issueid, orgsbatch) {\r\n        WebUtil.ajax({\r\n            url: \"GetOrgs\",\r\n            data: { orgcode: parentCode, issueid: issueid ,orgsbatch:orgsbatch},\r\n            success: function (data) {\r\n                $(obj).find(\"option\").each(function () { $(this).remove(); });\r\n                $(obj).append($(\"<option value='0'>--\u8bf7\u9009\u62e9--</option>\"));\r\n                if (data.result.length > 0) {\r\n                    for (var i = 0; i < data.result.length; i++) {\r\n                        var option = $(\"<option value='\" + data.result[i].orgCode + \"'>\" + data.result[i].orgName + \"</option>\");\r\n                        $(obj).append(option);\r\n                    }\r\n                }\r\n            },\r\n            error: function (data) {\r\n\r\n            }\r\n        });\r\n    }\r\n    var level = \"noend\";\r\n    var secondflag=false;\r\n\r\n    var globalorgdate;  // = null;\r\n    function DateDiff()\r\n    {\r\n\r\n        var now= new Date();\r\n        var sdaystr = now.getFullYear().toString() + \"/\" + (now.getMonth() + 1).toString() + \"/\" + now.getDate().toString();\r\n        var sday = new Date(sdaystr);\r\n\r\n        var edaystr = exchangetime.getFullYear().toString() + \"/\" + (exchangetime.getMonth() + 1).toString() + \"/\" + exchangetime.getDate().toString() ;\r\n        var eday = new Date(edaystr);\r\n        var ts = parseInt(eday - sday);\r\n        var days = parseInt(ts / 1000 / 60 / 60 / 24);\r\n        return days;\r\n    }\r\n\r\n\r\n    function createdate()\r\n    {\r\n        var ctx = new Date();\r\n        var y = ctx.getFullYear() + 1;\r\n\r\n        var dayspan =parseInt( DateDiff());\r\n        $('#coindate').datepicker({\r\n            changeMonth: true,\r\n            changeYear: true,\r\n            autoSize: true,\r\n            yearRange: (y - 100).toString() + \":\" + y.toString(),\r\n            dateFormat: $.datepicker.W3C,\r\n            defaultDate: dayspan,\r\n            beforeShowDay: isorderdate\r\n        });\r\n\r\n        $('#coindate').datepicker('option', $.datepicker.regional['zh-CN']);\r\n        $('#ui-datepicker-div').hide();\r\n    }\r\n\r\n    function createorg(parentCode, id, issueid, orgsbatch) {\r\n        globalissueid = issueid;\r\n        WebUtil.ajax({\r\n            url: \"GetOrgs\",\r\n            data: { orgcode: parentCode, issueid: issueid, orgsbatch: orgsbatch },\r\n            success: function (data) {\r\n                if (data.result.length > 0)\r\n                {\r\n                    level = \"notend\";\r\n\r\n                    var input = buildHTML(\"select\", {\r\n                        id: 'orglevel' + id,\r\n                        name: 'orglevel' + id\r\n                    });\r\n\r\n                    if ($(\"#orglevel\").children('#orglevel' + id).length == 0) {\r\n                        $('#orglevel').append(input);\r\n                    }\r\n\r\n                    $('#orglevel' + id).find(\"option\").each(function () { $(this).remove(); });\r\n                    $('#orglevel' + id).append($(\"<option value='0'>--\u8bf7\u9009\u62e9--</option>\"));\r\n                    var sel = $('#orglevel' + id).get(0);\r\n                    for (var i = 0; i < data.result.length; i++) {\r\n                        //var option = $(\"<option value='\" + data.result[i].orgCode + \"'>\" + data.result[i].orgName + \"</option>\");\r\n                        var newOption = document.createElement(\"OPTION\");\r\n                        newOption.text = data.result[i].orgName;\r\n                        newOption.value = data.result[i].orgCode;\r\n                        sel.options.add(newOption)\r\n                        //$('#orglevel' + id).append(option);\r\n                    }\r\n                    $('#orglevel' + id).change(function () {\r\n                        clearorglevel(id);\r\n                        createorg($('#orglevel' + id).val(), id + 1, issueid,orgsbatch);\r\n                        if (level == \"end\") {\r\n                            WebUtil.ajax({\r\n                                url: \"GetOrgProducts\",\r\n                                data: { issueid: issueid, orgcode: $('#orglevel' + id).val() },\r\n                                success: function (data) {\r\n                                    if (data.result.length > 0) {\r\n                                        createproduct(issueid, data.result);\r\n                                    }\r\n                                },\r\n                                error: function (data) {\r\n\r\n                                }\r\n                            });\r\n                            getorderdates(issueid, $('#orglevel' + id).val());\r\n\r\n                            $.each(globalorgdate, function (k, v)\r\n                            {\r\n                                if (k == \"99990101\")\r\n                                {\r\n                                    secondflag = true;\r\n                                    return false;\r\n                                }\r\n                            })\r\n\r\n                            if (!secondflag)\r\n                            {\r\n                                createdate();\r\n                                $(\"#noticeinfo\").hide();\r\n                                $(\"#tabledate\").show();\r\n                            }\r\n                            else\r\n                            {\r\n                                $(\"#noticeinfo\").show();\r\n                                $(\"#tabledate\").hide();\r\n                                $(\"#noticeinfo\").html(\"\u5151\u6362\u65f6\u95f4\u5f85\u4eba\u6c11\u94f6\u884c\u786e\u5b9a\u540e\u53e6\u884c\u901a\u77e5\");\r\n                                $(\"#coindate\").val(\"99990101\");\r\n                            }\r\n\r\n                        }\r\n\r\n\r\n\r\n                    })\r\n\r\n\r\n\r\n                }\r\n                else {\r\n                    level = \"end\";\r\n                }\r\n\r\n\r\n            },\r\n            error: function (data) {\r\n            }\r\n        });\r\n    }\r\n\r\n    function clearorglevel(id)\r\n    {\r\n\r\n        $(\"select[id^='orglevel']\").each(function (index) {\r\n            if(index>=id)\r\n            {\r\n                $(this).remove();\r\n            }\r\n        });\r\n\r\n    }\r\n    function getorderdates(globalissueid, globalorgcode)\r\n    {\r\n        WebUtil.ajax({\r\n            url: \"GetOrgDates\",\r\n            data: { issueid: globalissueid, orgcode: globalorgcode },\r\n            success: function (data) {\r\n                if (data.status == \"SUCCESS\") {\r\n                    globalorgdate = data.result;\r\n                }\r\n\r\n            },\r\n            error: function (data) {\r\n                //globalorgdate = null;   // {}\r\n            }\r\n        });\r\n    }\r\n\r\n    function isorderdate(date) {\r\n        var isOrdered = false;\r\n        var scheduleStatus = \"\u4e0d\u5728\u5151\u6362\u671f\";\r\n\r\n\r\n        $.each(globalorgdate, function (k, v) {\r\n            //\u5faa\u73af\u83b7\u53d6\u6570\u636e\r\n            if (date.Format(\"yyyyMMdd\") == k) {\r\n                if (v > 0) {\r\n                    isOrdered = true;\r\n                }\r\n                else {\r\n                    scheduleStatus = \"\u5df2\u6ee1\";\r\n                }\r\n\r\n                return false;\r\n\r\n            }\r\n\r\n        });\r\n\r\n        if (!isOrdered) {\r\n            return [false,'', scheduleStatus];\r\n        }\r\n        else {\r\n            return [true, '','\u53ef\u9009\u62e9'];\r\n        }\r\n\r\n    }\r\n\r\n    function nullTransfer(str) {\r\n        if (NaN == str || str == undefined || str == \"undefined\" || str == null || str.length == 0 || str == \"null\" || \"\" == str) {\r\n            return '\u679a';\r\n        }\r\n        return str;\r\n    }\r\n\r\n    function createproduct(issueid, orgproduct) {\r\n        WebUtil.ajax({\r\n            url: \"GetProducts\",\r\n            data: { issueid: issueid },\r\n            success: function (data) {\r\n                if (data.result.length > 0) {\r\n                    for (var i = 0; i < data.result.length; i++) {\r\n                        if (orgproduct == null) {\r\n                            var tr = buildHTML(\"tr\", {\r\n                                id: 'producttr' + i\r\n                            });\r\n                            if ($(\"#tableproduct\").children('#producttr' + i).length == 0) {\r\n                                $('#tableproduct').append(tr);\r\n                            }\r\n\r\n                            var nametd = buildHTML(\"td\", data.result[i].name, {\r\n                                id: 'producttd' + i + 1\r\n                            });\r\n\r\n                            $('#producttr' + i).append(nametd);\r\n\r\n\r\n                            var spinnertd = buildHTML(\"td\", {\r\n                                id: 'producttd' + i + 2\r\n                            });\r\n\r\n                            $('#producttr' + i).append(spinnertd);\r\n                            var spinner = buildHTML(\"input\", {\r\n                                type: 'text',\r\n                                id: 'cardvalue' + i,\r\n                                name: 'cardvalue' + i,\r\n                                value: data.result[i].personalMax\r\n                            });\r\n                            $('#producttd' + i + 2).append(spinner);\r\n                            var hidden = buildHTML(\"input\", {\r\n                                type: 'hidden',\r\n                                id: 'cardid' + i,\r\n                                name: 'cardid' + i,\r\n                                value: data.result[i].id\r\n                            });\r\n                            $('#producttd' + i + 2).append(hidden);\r\n\r\n                            $(\"#cardvalue\" + i).spinner({\r\n                                max: 1000,\r\n                                min: 1,\r\n                                step: 1\r\n                            });\r\n\r\n                            //var td = buildHTML(\"td\", '(\u9762\u503c' + data.result[i].price + '\u5143,\u6bcf\u4eba' + data.result[i].personalMin + \"-\" + data.result[i].personalMax + data.result[i].unit + ')', {\r\n                            var td = buildHTML(\"td\", '(\u6bcf\u4eba' + data.result[i].personalMin + \"-\" + data.result[i].personalMax + nullTransfer(data.result[i].unit) + ')', {\r\n\r\n                                id: 'producttd' + i + 3\r\n                            });\r\n\r\n                            $('#producttr' + i).append(td);\r\n                        }\r\n                        else {\r\n\r\n                            for (var j = 0; j < orgproduct.length; j++) {\r\n                                if (orgproduct[j].product == data.result[i].id) {\r\n                                    $('#producttd' + i + 3).html('(\u6bcf\u4eba' + data.result[i].personalMin + \"-\" + data.result[i].personalMax + nullTransfer(data.result[i].unit) + ')\u8fd8\u53ef\u9884\u7ea6' + orgproduct[j].quota + nullTransfer(data.result[i].unit));\r\n                                }\r\n                            }\r\n                        }\r\n\r\n\r\n                    }\r\n\r\n\r\n                }\r\n            },\r\n            error: function (data) {\r\n\r\n            }\r\n        })\r\n    }\r\n\r\n\r\n    buildHTML = function (tag, html, attrs) {\r\n        // you can skip html param\r\n        if (typeof (html) != 'string') {\r\n            attrs = html;\r\n            html = null;\r\n        }\r\n        var h = '<' + tag;\r\n        for (attr in attrs) {\r\n            if (attrs[attr] === false) continue;\r\n            h += ' ' + attr + '=\"' + attrs[attr] + '\"';\r\n        }\r\n        return h += html ? \">\" + html + \"</\" + tag + \">\" : \"/>\";\r\n    };\r\n\r\n</script>\r\n<script type=\"text/javascript\">\r\n\r\n        var degraded = \"false\";\r\n        var countdown = 60;\r\n        function settime(val, that) {\r\n            if (countdown == 0) {\r\n                that.removeAttr(\"disabled\");\r\n                that.attr(\"value\", \"\u83b7\u53d6\u9a8c\u8bc1\u7801\");\r\n                countdown = 60;\r\n            } else {\r\n                that.attr(\"disabled\", true);\r\n                that.attr(\"value\", \"\u91cd\u65b0\u53d1\u9001(\" + countdown + \")\");\r\n                countdown--;\r\n                setTimeout(function () {\r\n                    settime(val, that);\r\n                }, 1000);\r\n            }\r\n        }\r\n\r\n        function SetEventBind(){\r\n            $(\"#sendValidate\").live(\"click\", function () {\r\n                if ($(this).attr(\"disabled\") == true) {\r\n                }\r\n                else {\r\n                    var phoneNumber = $('#mobile').val();\r\n                    var regex = /^(1[1-9][0-9])\\d{8}$/;\r\n                    if (!regex.test(phoneNumber)) {\r\n                        alert(\"\u8bf7\u8f93\u5165\u6b63\u786e\u7684\u624b\u673a\u53f7\u7801\uff01\");\r\n                    }\r\n                    else {\r\n                        submitPicCapcha();\r\n                    }\r\n                }\r\n            });\r\n        }\r\n\r\n\r\n        function submitPicCapcha() {\r\n            var ccode = \"\";\r\n            var piccode = $(\"[name='piccode']\").val();\r\n            if (piccode == \"\")\r\n            {\r\n                alert(\"\u8bf7\u8f93\u5165\u56fe\u5f62\u9a8c\u8bc1\u7801\uff01\");\r\n            }\r\n            else\r\n            {\r\n                var phoneNumber = $('#mobile').val();\r\n                var msg;\r\n                $.ajax({\r\n                    type: \"POST\",\r\n                    contentType: \"application/json; charset=utf-8\",\r\n                    url: \"../Coin/SendCaptchaCode?mobile=\" + phoneNumber + \"&piccode=\" + piccode,\r\n                    dataType: 'json',\r\n                    success: function (data) {\r\n                        switch (data.result) {\r\n                            case 0:\r\n                                msg = \"\u77ed\u4fe1\u9a8c\u8bc1\u7801\u5df2\u53d1\u9001\u6210\u529f\";\r\n                                $(\"#content\").hide();\r\n                                $(\"#mask\").hide();\r\n                                $(\"#captchacodeError\").removeClass(\"RedFont\").addClass(\"GreenFont\");\r\n                                $(\"#captchacodeError\").html(msg);\r\n                                countdown = 60;\r\n                                //\u6309\u94ae\u663e\u793a\u5012\u8ba1\u65f6\r\n                                var that = $('#sendValidate');\r\n                                settime(countdown, that);\r\n                                //60\u79d2\u540e\u91cd\u65b0\u83b7\u53d6;\r\n                                break;\r\n                            case 1:\r\n                                msg = \"\u4e24\u6b21\u53d1\u9001\u95f4\u9694\u4e0d\u80fd\u4f4e\u4e8e\u4e00\u5206\u949f\";\r\n                                // $(\"#picError\").removeClass(\"GreenFont\").addClass(\"RedFont\");\r\n                                break;\r\n                            case 2:\r\n                                msg = \"\u77ed\u4fe1\u9a8c\u8bc1\u7801\u53d1\u9001\u5931\u8d25\";\r\n                                //$(\"#picError\").removeClass(\"GreenFont\").addClass(\"RedFont\");\r\n                                break;\r\n                            case 3:\r\n                                msg = \"\u56fe\u5f62\u9a8c\u8bc1\u7801\u9519\u8bef\";\r\n                                //$(\"#picError\").removeClass(\"GreenFont\").addClass(\"RedFont\");\r\n                                break;\r\n