<Window x:Class="WPF_Miao.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPF_Miao"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Window.Resources>
        <Style TargetType="Button">
            <Setter Property="Width" Value="80" />
            <Setter Property="Height" Value="30" />
        </Style>
    </Window.Resources>
    <Grid>
        <StackPanel>
            <Button Content="滇医通" Click="DianYiTong_Click"></Button>
            <Button Content="点击医院" Click="Hospital_Click"></Button>
            <Button Content="SubmitOrder" Click="SubmitOrder_Click"></Button>
        </StackPanel>
    </Grid>
</Window>
