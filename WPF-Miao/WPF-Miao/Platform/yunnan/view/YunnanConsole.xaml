<Window x:Class="WPF_Miao.Platform.yunnan.view.YunnanConsole"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local="clr-namespace:WPF_Miao.Platform.yunnan.view"
        xmlns:controls="clr-namespace:CoreControl.LogConsole;assembly=CoreControl"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        Title="YunnanConsole" 
        Height="800" Width="800" 
        WindowState="Normal">
    <DockPanel>
        <Grid DockPanel.Dock="Top" Height="30" Background="SkyBlue">

        </Grid>
        <Grid DockPanel.Dock="Right" x:Name="console" HorizontalAlignment="Left">
            <Button Command="{Binding AppointmentCommand}">
                
            </Button>
        </Grid>
        <Grid DockPanel.Dock="Left" x:Name="logs" Width="300">
            <controls:LogPanel WriteLineAction="{Binding WriteLineAction, Mode=OneWayToSource}">

            </controls:LogPanel>
        </Grid>
    </DockPanel>
</Window>
