<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Style TargetType="TextBlock" x:Key="TitleH3" BasedOn="{StaticResource MaterialDesignBody1TextBlock}" >
        <Setter Property="FontWeight" Value="Bold" />
        <Setter Property="Foreground" Value="#FFCD5D3A" />
        <Setter Property="Padding" Value="5,0" />
    </Style>

    <Style TargetType="ComboBox" BasedOn="{StaticResource MaterialDesignOutlinedComboBox}" x:Key="mui_combobox">
        <Setter Property="HorizontalAlignment" Value="Left" />
        <Setter Property="VerticalContentAlignment" Value="Center" />
        <Setter Property="Margin" Value="2" />
        <Setter Property="Padding" Value="2,6,6,6" />
        <Setter Property="Height" Value="36" />
    </Style>
</ResourceDictionary>